<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.mappers.meetingRoomEquipmentMapper">
	
	<select id="listMeetingRoomEquipment" resultType="MeetingRoomEquipment">
		SELECT * FROM meeting_room_equipment
	</select>
	
	<insert id="insertMeetingRoomEquipment" parameterType="MeetingRoomEquipment">
		INSERT INTO meeting_room_equipment VALUES (#{equipmentId}, #{meetingRoomId})
	</insert>
	
	<select id="selectMeetingRoomEquipmentById" resultType="MeetingRoomEquipment">
		SELECT * FROM meeting_room_equipment WHERE meeting_room_id = #{meetingRoomId}
	</select>
</mapper>